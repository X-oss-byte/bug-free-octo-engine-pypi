  "version": "0.2.0",
  "configurations": [
    {
      "name": "Basic Turbo Build",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/cli/cmd/turbo",
      "cwd": "${workspaceRoot}/examples/basic",
      "args": ["run", "build"]
    },
    {
      "name": "Turbo Version",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceRoot}/cli/cmd/turbo",
      "cwd": "${workspaceRoot}/examples/basic",
      "args": ["--version"]
    }
  ]
}
