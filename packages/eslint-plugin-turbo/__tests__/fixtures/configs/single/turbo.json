{
  "globalEnv":[
    "NEW_STYLE_GLOBAL_ENV_KEY",
    "$NEW_STYLE_GLOBAL_ENV_KEY"
  ],
  "globalDependencies":[
    "$GLOBAL_ENV_KEY"
  ],
  "pipeline":{
    "test":{
      "outputs":[
        "coverage/**"
      ],
      "dependsOn":[
        "^build"
      ]
    },
    "lint":{
      "outputs":[]
    },
    "dev":{
      "cache":false
    },
    "build":{
      "outputs":[
        "dist/**/*",
        ".next/**/*"
      ],
      "env":[
        "NEW_STYLE_ENV_KEY"
      ],
      "dependsOn":[
        "^build",
        "$TASK_ENV_KEY",
        "$ANOTHER_ENV_KEY"
      ]
    }
  }
}
